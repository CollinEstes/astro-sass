  "bugs": {
    "url": "https://github.com/MikeFielden/astro-sass/issues"
  },
  "homepage": "https://github.com/MikeFielden/astro-sass#readme",
  "dependencies": {
    "node-sass": "^3.3.2"
  },
  "devDependencies": {
    "astro": "^0.1.6"
  }
}
